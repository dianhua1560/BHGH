<style type = 'text/css'>
.event-photos-div{
    margin-top:15px;
}
</style>

<div class = 'col-md-3'>
</div>
<div class = 'col-md-6'>
<h1><%= @event.title %></h1>
<%= link_to "Back to Events", events_path %>
<div>
    <%= @event.description %>
</div>
<div>
    Time: <%= @event.time_string %>
</div>
<div>
    Organizer: <%= @event.organizer %>
</div>
<% if @event.location %>
    <div>
        Location: <%= @event.location %>
    </div>
<% end %>
<div class = 'event-photos-div'>
<% @photos.each do |photo| %>
    <img src = '<%= photo.url %>' height = 200/>
<% end %>
</div>

</div>
<div class = 'col-md-3'></div>
